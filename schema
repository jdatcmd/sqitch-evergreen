#!/bin/sh
 
PGUSER=evergreen sqitch add schema.permissions --requires create_extensions -n 'perm schema' 
PGUSER=evergreen sqitch add schema.query --requires create_extensions -n 'query schema'
PGUSER=evergreen sqitch add schema.biblio --requires create_extensions -n 'biblio schema'
PGUSER=evergreen sqitch add schema.authority --requires create_extensions -n 'authority schema'
PGUSER=evergreen sqitch add schema.vandelay --requires create_extensions -n 'vandelay schema'
PGUSER=evergreen sqitch add schema.staging --requires create_extensions -n 'staging schema'
PGUSER=evergreen sqitch add schema.functions --requires create_extensions -n 'functions schema'
PGUSER=evergeen sqitch add schema.metabib --requires create_extensions -n 'metabib schema' 
PGUSER=evergeen sqitch add schema.asset --requires create_extensions -n 'asset schema' 
PGUSER=evergeen sqitch add schema.container --requires create_extensions -n 'container schema' 
PGUSER=evergeen sqitch add url_verify.schema --requires create_extensions -n 'url_verify schema' 
PGUSER=evergeen sqitch add url_verify.functions --requires url_verify.schema -n 'url_verify functions' 

